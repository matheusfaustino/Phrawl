{
  "version": "dev-master",
  "name": "matheusfaustino/phrawl",
  "description": "A web crawling framework in PHP ",
  "readme": "README.md",
  "type": "library",
  "authors": [
    {
      "name": "Matheus Faustino",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/matheusfaustino/Phrawl/issues"
  },
  "keywords": [
    "crawler",
    "php",
    "crawling",
    "wip",
    "crawling-framework",
    "scraper"
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "amphp/amp": "^2.1",
    "amphp/artax": "^3.0",
    "amphp/parallel-functions": "^0.1.3",
    "monolog/monolog": "^1.23",
    "symfony/css-selector": "^4.2",
    "symfony/panther": "^0.2.0"
  },
  "autoload": {
    "psr-4": {
      "Phrawl\\": "./src"
    }
  },
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.2"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "bin": [
    "phrawl"
  ]
}
